{
  "name": "@capire/sflight",
  "version": "1.0.0",
  "description": "CAP flight demo scenario",
  "repository": "https://github.com/SAP-samples/cap-sflight",
  "dependencies": {
    "@sap/audit-logging": "^5.5.3",
    "@sap/cds": "^5.9.5",
    "@sap/xsenv": "^3.2.2",
    "@sap/xssec": "^3.2.13",
    "express": "^4.18.1",
    "hdb": "^0.18.3",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jest": "^27.5.1",
    "qunit": "^2.19.1",
    "sqlite3": "^5.0.8"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "cds run",
    "test": "jest --runInBand"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[development]": {
          "strategy": "dummy"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "log": {
      "service": true
    },
    "features": {
      "preserve_computed": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 20000
  },
  "license": "SAP SAMPLE CODE LICENSE",
  "private": true
}
